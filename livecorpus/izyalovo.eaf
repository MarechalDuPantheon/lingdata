    <TIER ANNOTATOR="MarechalDuPantheon" DEFAULT_LOCALE="en"
        LINGUISTIC_TYPE_REF="translation" PARENT_REF="text@pns1926f"
        PARTICIPANT="@pns1926f" TIER_ID="transl@pns1926f">
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a31" ANNOTATION_REF="a1">
                <ANNOTATION_VALUE>Da war es am Morgen.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a32" ANNOTATION_REF="a2">
                <ANNOTATION_VALUE>Jäh ritten sie zu Pferden.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a33" ANNOTATION_REF="a3">
                <ANNOTATION_VALUE>Die Pferde waren groß, ihre Schweife waren kurz.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a34" ANNOTATION_REF="a4">
                <ANNOTATION_VALUE>Drei Pferde rannten durch das Dorf von Golotschölow, aus dem Wald und hier zu uns.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a35" ANNOTATION_REF="a5">
                <ANNOTATION_VALUE>Und mein Vati sagte: Das sind ja Deutsche!</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a36" ANNOTATION_REF="a6">
                <ANNOTATION_VALUE>Und mein Vati diente vor dem, vor der Armee, vor der Heirat, früher dientet ihr nämlich lange, und er diente für neun Jahre in Deutschland.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a37" ANNOTATION_REF="a7">
                <ANNOTATION_VALUE>Weil er wahrscheinlich in Kriegsgefangenschaft geraten wurde, ein Krieg war damals.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a38" ANNOTATION_REF="a9">
                <ANNOTATION_VALUE>Na.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a39" ANNOTATION_REF="a10">
                <ANNOTATION_VALUE>Etwas verstand er auf Deutsch.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a40" ANNOTATION_REF="a11">
                <ANNOTATION_VALUE>Sie fingen Hähnchen.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a41" ANNOTATION_REF="a12">
                <ANNOTATION_VALUE>Wir hatten Kühe, Hähnchen, Gänse.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a42" ANNOTATION_REF="a13">
                <ANNOTATION_VALUE>Sie fingen Gänse, zupften ab, versengten die ein bisschen und legten in den Gusseisentopf, so aßen sie Fleisch. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a43" ANNOTATION_REF="a14">
                <ANNOTATION_VALUE>Es ist, wie das Zigeunern machen.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a44" ANNOTATION_REF="a15">
                <ANNOTATION_VALUE>Barbaren.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a45" ANNOTATION_REF="a16">
                <ANNOTATION_VALUE>Es gab Bienen mit zwei Bienenstöcken, sie beschütteten sie mit Wasser, aber nahmen Honig mit.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a46" ANNOTATION_REF="a17">
                <ANNOTATION_VALUE>Die Österreicher waren sehr böse, böser als die Deutsche.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a47" ANNOTATION_REF="a18">
                <ANNOTATION_VALUE>Oh, die Österreicher waren böse.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a48" ANNOTATION_REF="a19">
                <ANNOTATION_VALUE>Und die Österreicher hatten auch #unklar#, auch #unklar#, das eine Farbe hatte, wie es bei einer Kamille gibt, auf ihren Garnisonsmützen, dieses Eisernes wurde angeheftet. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a49" ANNOTATION_REF="a20">
                <ANNOTATION_VALUE>Na, sie waren sehr böse.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a50" ANNOTATION_REF="a21">
                <ANNOTATION_VALUE>Doch erinnere ich mich, das passierte in der Nizhnyaya in Izyalovo, die befindet sich unten, die Deutsche waren auf dem Feld von #unklar#.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a51" ANNOTATION_REF="a22">
                <ANNOTATION_VALUE>Und dort kamen sie zu den Mädchen. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a52" ANNOTATION_REF="a23">
                <ANNOTATION_VALUE>Die Mädchen versteckten sich hinter dem Herd, doch war die Bank, und sie versteckten sich unter dieser Bank, die Deutsche zogen sie von dort mit den Feuerhaken hinaus.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a53" ANNOTATION_REF="a24">
                <ANNOTATION_VALUE>Verdammte Scheusale.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a54" ANNOTATION_REF="a25">
                <ANNOTATION_VALUE>Solche waren diese Schufte, diese Teufel.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a55" ANNOTATION_REF="a26">
                <ANNOTATION_VALUE>So war es auch.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a56" ANNOTATION_REF="a27">
                <ANNOTATION_VALUE>Und die Deutsche kamen, nahmen Kühe und sonst was, was wer hatte, weg.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a57" ANNOTATION_REF="a28">
                <ANNOTATION_VALUE>Und die Unsere kamen, wir hatten zwei Ferkelchen, der Vorsitzende brachte sie, und die Unsere nahmen zwei Ferkel mit.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a58" ANNOTATION_REF="a29">
                <ANNOTATION_VALUE>Die Ferkel.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a59" ANNOTATION_REF="a30">
                <ANNOTATION_VALUE>Alle verdienten gegenseitig während des Krieges.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
    </TIER>
